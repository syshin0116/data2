package com.hi.mvcProject;

public class CastingTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 127; //4파이트(큰)
		byte b = (byte) a; //1바이트(작은)
		
		byte c = 127;
		int d = c;
		
		double a2 = 100.001;
		float b2 = (float)a2;
		
		float c2 = 100.001;
		double d2 = a2;
	}

}
