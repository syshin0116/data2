package com.hi.mvcProject;

import java.util.HashMap;

public class CastingTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 127; //4파이트(큰)
		byte b = (byte) a; //1바이트(작은)
		
		byte c = 127;
		int d = c;
		
		double a2 = 100.001;
		float b2 = (float)a2;
		
		float c2 = 100.001;
		double d2 = a2;
		
		//자바슼크립트에서 json과 같은 역할 키: 값 저장구조
		HashMap map = new HashMap();
		map.put("name", "홍길동");
		map.put("age", 100);
		
		String name = (String) map.get("name");
		int age = (int) map.get("age");
		//int <--> Integer <-(강제형변환)- Object
		//참조형 형변환은 상속관계에 있을때만 가능
		//String--Integer(형제관계에서는 형변환 사용불가)
//		String ag2 = age;
		//int <--> Integer, float<-->Float, double <--> Double
	}

}
